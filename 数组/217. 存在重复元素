    // }
    // return flag
    // 3、Set
        let flag = false;
    if(nums.length < 2) return flag
    const set = new Set()
    for(let i = 0;i<nums.length;i++){
        if(set.has(nums[i])){
            flag = true;
            return flag
        }else{
            set.add(nums[i])
        }
    }
    return flag
};
